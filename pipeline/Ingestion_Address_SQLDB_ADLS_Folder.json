{
	"name": "Ingestion_Address_SQLDB_ADLS_Folder",
	"properties": {
		"description": "copying address table data in csv in address directory",
		"activities": [
			{
				"name": "copying_address_table_to_address_dir",
				"description": "address data table data to address directory in landing",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
